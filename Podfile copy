# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'DoctorDesktop' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Doctor DeskTop
 
  pod 'SideMenu'
  pod 'IQKeyboardManagerSwift'
  pod 'Alamofire'
  pod 'MaterialComponents/Tabs'
  pod 'SwinjectStoryboard'

  pod 'DropDown'
  pod 'SCLAlertView' , :git => 'https://github.com/vikmeup/SCLAlertView-Swift'
  pod 'ObjectMapper'
  pod 'NVActivityIndicatorView'
  pod 'Toast-Swift', :inhibit_warnings => true
  pod 'SearchTextField', :inhibit_warnings => true
  pod 'KMPlaceholderTextView'
  pod 'VHBoomMenuButton/Swift'
  pod 'Swinject'
  pod 'UICollectionViewLeftAlignedLayout'
  pod 'Kingfisher'
  pod 'PocketSVG', :inhibit_warnings => true
  pod 'SwiftyBeaver'
  pod 'SwiftyJSON'
  pod 'Reachability'
  pod 'MZFormSheetController', '~> 3.1'
  pod 'MOLH'
  pod 'OhhAuth'
  pod 'OAuthSwift', '~> 1.2.0'
  pod 'Stuff/Codable'
  pod 'PopupDialog'
end
post_install do |installer|
  installer.pods_project.targets.each do |target|
    target.build_configurations.each do |config|
      config.build_settings['IPHONEOS_DEPLOYMENT_TARGET'] = '12.0'
    end
  end
end
